{
  "expo": {
    "name": "EmoGo",
    "slug": "emogo1124",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android": {
      "package": "com.emogo.datacollection",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 3
    },
    "ios": {
      "bundleIdentifier": "com.emogo.datacollection"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow EmoGo to access your camera to record vlogs"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow EmoGo to access your location"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "25b852df-2c45-49cf-83ea-821146b062e5"
      }
    },
    "owner": "shane5658"
  }
}
